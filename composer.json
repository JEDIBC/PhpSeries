{
    "name"        : "jedibc/phpseries",
    "description" : "PHP client library of the BetaSeries web api",
    "keywords"    : [
        "betaseries",
        "tvshows"
    ],
    "type"        : "library",
    "license"     : "MIT",
    "authors"     : [
        {
            "name"    : "Bruno CHALOPIN",
            "homepage": "https://github.com/JEDIBC"
        }
    ],
    "require"     : {
        "php"                     : ">=5.5",
        "psr/log"                 : "~1.0",
        "guzzlehttp/guzzle"       : "~3.8",
        "symfony/options-resolver": "~2.6"
    },
    "require-dev" : {
        "phpunit/phpunit"               : "~4.0",
        "m6web/Symfony2-coding-standard": "~1.1",
        "m6web/coke"                    : "~1.0",
        "mockery/mockery"               : "~0.9"
    },
    "autoload"    : {
        "psr-4": {
            "PhpSeries\\": "src/PhpSeries/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config"      : {
        "bin-dir": "bin"
    }
}